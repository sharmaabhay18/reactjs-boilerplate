{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": ["src/*"],
      "assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "actions/*": ["src/actions/*"],
      "reducers/*": ["src/reducers/*"],
      "stylesheets/*": ["src/stylesheets/*"],
      "pages/*": ["src/pages/*"],
      "utils/*": ["src/utils/*"]
    }
  },
  "exclude": ["node_modules"]
}
